      "meta:xdmType": "string",
      "meta:xdmField": "xdm:preferredDepartureAirportCode",
      "tagNumber": 4
    },
    "ticketDelivery": {
      "title": "Ticket Delivery",
      "type": "string",
      "enum": [
        "eTicket",
