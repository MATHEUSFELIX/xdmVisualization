{
  "id": "_xdm.mixins.profile.profile-user-account-details",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "User Account Details",
  "type": "object",
  "meta:extensible": true,
  "meta:abstract": true,
  "meta:tags": {
    "industry": [
      "retail",
      "financial_services",
      "travel_and_hospitality",
      "telecom",
      "automotive",
      "education",
      "health_and_life_sciences",
      "high_tech",
      "manufacturing",
      "media_and_entertainment",
      "public_sector"
    ]
  },
  "description": "",
  "meta:status": "experimental",
  "meta:xdmType": "object",
  "properties": {
    "userAccount": {
      "type": "object",
      "meta:xdmType": "object",
      "properties": {
        "ID": {
          "title": "ID",
          "type": "string",
          "description": "Captures the user account ID.",
          "meta:xdmType": "string",
          "meta:xdmField": "xdm:ID",
          "tagNumber": 1
        },
        "contactDetails": {
          "title": "Contact Details",
          "$ref": "profile-personal-details.schema.json",
          "description": "Extension of the Personal Profile data type.",
          "meta:xdmField": "xdm:contactDetails",
          "tagNumber": 2
        },
        "startDate": {
          "title": "Start Date",
          "type": "string",
          "format": "date-time",
          "description": "Date when the account was opened.",
          "meta:xdmType": "date-time",
          "meta:xdmField": "xdm:startDate",
          "tagNumber": 3
        },
        "status": {
          "title": "Status",
          "type": "string",
          "enum": [
            "active",
            "inactive",
            "disabled",
            "other"
          ],
          "description": "This should describe the current status of the account",
          "meta:xdmType": "string",
          "meta:xdmField": "xdm:status",
          "tagNumber": 4
        },
        "accountType": {
          "title": "Account Type",
          "type": "string",
          "description": "Captures the user account type, if applicable.",
          "meta:xdmType": "string",
          "meta:xdmField": "xdm:accountType",
          "tagNumber": 5
        },
        "preferences": {
          "title": "Preferences",
          "type": "array",
          "description": "M/F, other shopping preferences. Preferences in personal profile is already captured- things like perferred method of contact, not shopping prefs. ",
          "meta:xdmType": "array",
          "items": {
            "type": "string",
            "meta:xdmType": "string",
            "tagNumber": 1
          },
          "meta:xdmField": "xdm:preferences",
          "tagNumber": 6
        }
      },
      "meta:xdmField": "xdm:userAccount",
      "tagNumber": 1
    }
  },
  "meta:xdmId": "https://ns.adobe.com/xdm/mixins/profile/profile-user-account-details",
  "meta:altId": "_xdm.mixins.profile.profile-user-account-details",
  "id": "_xdm.mixins.profile.profile-user-account-details"
}
