{
    "$id": "https://ns.adobe.com/xdm/context/search",
    "$schema": "http://json-schema.org/draft-06/schema#",
    "title": "Search",
    "type": "object",
    "meta:extensible": true,
    "description": "Information about web search activity.",
    "definitions": {
        "search": {
            "properties": {
                "searchEngine": {
                    "title": "Search Engine",
                    "type": "string",
                    "description": "The search engine used by the search.",
                    "meta:xdmField": "xdm:searchEngine"
                },
                "searchEngineID": {
                    "title": "Search Engine ID",
                    "type": "string",
                    "format": "uri",
                    "description": "The application-specified identifier used to identify the search engine used by the search.",
                    "meta:xdmField": "xdm:searchEngineID"
                },
                "keywords": {
                    "title": "Keywords",
                    "type": "string",
                    "description": "Keywords for the search.",
                    "meta:xdmField": "xdm:keywords"
                },
                "isPaid": {
                    "title": "Is Paid",
                    "type": "boolean",
                    "description": "Indicate if the search is paid or not.",
                    "meta:xdmField": "xdm:isPaid"
                },
                "pageDepth": {
                    "title": "Page Depth",
                    "type": "integer",
                    "description": "Page depth in the search results.",
                    "meta:xdmField": "xdm:pageDepth"
                },
                "slot": {
                    "title": "Page Slot",
                    "type": "string",
                    "description": "Named section of the page where the search result appeared. For eg. Top, Side.",
                    "meta:enum": {
                        "top": "Top of the page",
                        "side": "Side of the page"
                    },
                    "meta:xdmField": "xdm:slot"
                },
                "position": {
                    "title": "Listing Position",
                    "type": "integer",
                    "description": "Position (rank) of the listing in the search result page.",
                    "meta:xdmField": "xdm:position"
                }
            }
        }
    },
    "allOf": [
        {
            "$ref": "https://ns.adobe.com/xdm/common/extensible#/definitions/@context"
        },
        {
            "$ref": "#/definitions/search"
        }
    ],
    "meta:status": "stabilizing",
    "meta:xdmId": "https://ns.adobe.com/xdm/context/search",
    "meta:altId": "_xdm.context.search"
}